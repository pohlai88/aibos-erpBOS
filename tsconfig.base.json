{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@aibos/contracts/*": ["packages/contracts/*"],
      "@aibos/api-client/*": ["packages/api-client/*"],
      "@aibos/services/*": ["packages/services/*"]
    }
  }
}
